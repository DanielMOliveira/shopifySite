iv>\n    </section>\n</div>\n\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\n<script>\n    $(document).ready(function () {\n        var markers = $(\".marker\");\n        var ns = \"http://www.w3.org/2000/svg\";\n\n        function randomRange(min, max) {\n            return Math.random() * (max - min) + min;\n        }\n\n        function generateSmoothOvalPath() {\n            const cx = 60;\n            const cy = 30;\n            const base_rx = randomRange(35, 45);\n            const base_ry = randomRange(15, 25);\n            const wobble = 1.5;\n            const segments = 8;\n\n            let angleStep = (Math.PI * 2) / segments;\n            let points = [];\n\n            for (let i = 0; i < segments; i++) {\n                let angle = i * angleStep;\n                let x = cx + Math.cos(angle) * (base_rx + randomRange(-wobble, wobble));\n                let y = cy + Math.sin(angle) * (base_ry + randomRange(-wobble, wobble));\n                points.push({ x, y, angle });\n            }\n\n            let endPoint = { ...points[0] };\n\n            if (Math.random() < 0.5) {\n                const imperfectionScale = randomRange(3, 7);\n                const perpendicularShift = randomRange(-4, 4);\n\n                const lastGeneratedPoint = points[segments - 1];\n                const firstPoint = points[0];\n\n                let dx = firstPoint.x - lastGeneratedPoint.x;\n                let dy = firstPoint.y - lastGeneratedPoint.y;\n                let len = Math.sqrt(dx * dx + dy * dy);\n\n                if (len > 0) {\n                    dx /= len;\n                    dy /= len;\n\n                    endPoint.x = firstPoint.x + dx * imperfectionScale;\n                    endPoint.y = firstPoint.y + dy * imperfectionScale;\n\n                    endPoint.x += -dy * perpendicularShift;\n                    endPoint.y += dx * perpendicularShift;\n                }\n            }\n\n            points.push(endPoint);\n\n            let pathString = `M${points[0].x},${points[0].y} `;\n\n            for (let i = 0; i < segments; i++) {\n                let p0 = points[i];\n                let p1 = points[i + 1];\n\n                let cp1x = p0.x + Math.cos(p0.angle + Math.PI / 2) * (base_rx / 3);\n                let cp1y = p0.y + Math.sin(p0.angle + Math.PI / 2) * (base_ry / 3);\n\n                let cp2x = p1.x + Math.cos(p1.angle - Math.PI / 2) * (base_rx / 3);\n                let cp2y = p1.y + Math.sin(p1.angle - Math.PI / 2) * (base_ry / 3);\n\n                pathString += `C${cp1x},${cp1y} ${cp2x},${cp2y} ${p1.x},${p1.y} `;\n            }\n\n            return pathString.trim();\n        }\n\n        markers.each(function () {\n            var marker = $(this);\n            var color = marker.attr(\"data-color\") || \"#f40d0b\";\n\n            marker.css(\"color\", color);\n\n            var svg = document.createElementNS(ns, \"svg\");\n            svg.setAttribute(\"viewBox\", \"0 0 120 60\");\n            svg.setAttribute(\"preserveAspectRatio\", \"none\");\n\n            var path = document.createElementNS(ns, \"path\");\n            path.setAttribute(\"stroke\", color);\n\n            // ⚠️ GERA o traço apenas UMA VEZ e aplica logo\n            var randomPath = generateSmoothOvalPath();\n            path.setAttribute(\"d\", randomPath);\n\n            // Estilo inicial do path (traço escondido)\n            path.setAttribute(\"fill\", \"none\");\n            path.setAttribute(\"stroke-width\", \"2\");\n            path.setAttribute(\"stroke-dasharray\", \"1000\");\n            path.setAttribute(\"stroke-dashoffset\", \"1000\");\n            path.style.transition = \"stroke-dashoffset 0.6s ease\";\n\n            svg.appendChild(path);\n            marker.append(svg);\n\n            // Apenas manipula a visibilidade do traço no hover\n            marker.on(\"mouseenter focus\", function () {\n                $(this).find(\"path\").css(\"stroke-dashoffset\", \"0\");\n            });\n\n            marker.on(\"mouseleave blur\", function () {\n                $(this).find(\"path\").css(\"stroke-dashoffset\", \"1000\");\n            });\n        });\n    });\n</script>",
