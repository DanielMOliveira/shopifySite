    "custom_liquid_mWAVd": {
      "type": "custom-liquid",
      "disabled": false,
      "settings": {
        "custom_liquid": "<style>\n    #zyBlogPosts {\n        overflow: hidden;\n        position: relative;\n        margin-top: 3rem;\n        width: 100%;\n        border-bottom: #e5e7eb 1px solid;\n    }\n\n\n    #zyBlogPosts .d-flex-blog {\n        display: flex;\n        align-items: center;\n        gap: 1rem;\n        margin-left: 2.5vw;\n        width: 95%;\n        margin-bottom: 1rem;\n    }\n\n    #zyBlogPosts .blog-posts-wrapper {\n        padding-top: 1rem;\n    }\n\n    #zyBlogPosts .blog-posts-wrapper h3 {\n        font-size: 2rem;\n        letter-spacing: 0.038rem;\n        font-weight: 500;\n    }\n\n    #zyBlogPosts .blogbtn {\n        background-color: transparent;\n        font-size: 0.875rem;\n        border-radius: 3rem;\n        border: #000 solid 1px;\n        color: #000;\n        padding: 0.4rem 0.8rem;\n        font-weight: 400;\n    }\n\n    #zyBlogPosts .carousel-container {\n        display: flex;\n        overflow-x: auto;\n        scroll-snap-type: x mandatory;\n        gap: 1rem;\n        scroll-padding-left: 2.5%;\n        -webkit-overflow-scrolling: touch;\n        scrollbar-width: none;\n        margin-bottom: 4rem;\n    }\n\n    #zyBlogPosts .carousel-container::-webkit-scrollbar {\n        display: none;\n    }\n\n    #zyBlogPosts .carousel-item {\n        flex: 0 0 300px;\n        scroll-snap-align: start;\n        overflow: hidden;\n        text-align: left;\n        padding-top: 1rem;\n        width: 100%;\n        height: auto;\n    }\n\n    #zyBlogPosts .carousel-item a {\n        text-decoration: none;\n        color: inherit;\n        overflow: hidden;\n    }\n\n    #zyBlogPosts .carousel-item .image-wrapper {\n        overflow: hidden;\n        width: 100%;\n        height: auto;\n    }\n\n    #zyBlogPosts .carousel-item img {\n        width: 100%;\n        height: auto;\n        object-fit: cover;\n        transition: transform 0.3s ease-in-out;\n    }\n\n    #zyBlogPosts .carousel-item img:hover {\n        transform: scale(1.02);\n    }\n\n    #zyBlogPosts .carousel-item h3 {\n        font-size: 1rem;\n        letter-spacing: 0.018rem;\n        margin: 0.6rem 0;\n        font-weight: 500;\n        text-transform: none;\n        line-height: 1.5;\n    }\n\n    #zyBlogPosts .carousel-button {\n        position: absolute;\n        top: 20px;\n        transform: translateY(0%);\n        border: 1px solid #ccc;\n        border-radius: 50%;\n        padding: 0.5rem;\n        cursor: pointer;\n        z-index: 1;\n        width: 32px;\n        height: 32px;\n    }\n\n    #zyBlogPosts .carousel-button svg {\n        width: 100%;\n        height: 100%;\n        color: #000;\n    }\n\n    #zyBlogPosts .carousel-button.left {\n        right: 70px;\n    }\n\n    #zyBlogPosts .carousel-button.right {\n        right: 30px;\n    }\n\n    #zyBlogPosts .carousel-item:first-child {\n        margin-left: 5%;\n    }\n\n    #zyBlogPosts .carousel-item:last-child {\n        margin-right: 5%;\n    }\n\n    @media (max-width: 768px) {\n\n        #zyBlogPosts .carousel-container {\n            margin-bottom: 1rem;\n        }\n\n        #zyBlogPosts .carousel-button {\n            display: none;\n        }\n\n        #zyBlogPosts {\n            padding-top: 1rem;\n        }\n\n        #zyBlogPosts .carousel-item:first-child {\n            margin-left: 5%;\n        }\n\n        #zyBlogPosts .carousel-item {\n            flex: 0 0 250px;\n        }\n\n        #zyBlogPosts {\n            margin-top: 2rem;\n            width: 100%;\n        }\n\n    }\n</style>\n\n<div id=\"zyBlogPosts\">\n    <div class=\"blog-posts-wrapper\">\n        <div class=\"d-flex-blog\">\n            <h3>Magazine</h3>\n            <a href=\"/pt/blogs/magazine\">\n                <button class=\"blogbtn\">ver tudo</button>\n            </a>\n        </div>\n\n        <div class=\"carousel-button left\" onclick=\"scrollCarousel(-1)\">\n            <svg class=\"icon chevron-left\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 16 16\" fill=\"none\">\n                <path fill-rule=\"evenodd\" clip-rule=\"evenodd\"\n                    d=\"M5.52852 8.47148C5.26819 8.21115 5.26819 7.78902 5.52852 7.52868L9.29975 3.75744C9.56007 3.49711 9.98218 3.49711 10.2425 3.75744C10.5029 4.01778 10.5029 4.4399 10.2425 4.70024L6.94269 8.00008L10.2425 11.2999C10.5029 11.5603 10.5029 11.9824 10.2425 12.2427C9.98218 12.5031 9.56007 12.5031 9.29975 12.2427L5.52852 8.47148Z\"\n                    fill=\"currentColor\" />\n            </svg>\n        </div>\n\n        <div class=\"carousel-button right\" onclick=\"scrollCarousel(1)\">\n            <svg class=\"icon chevron-right\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 16 16\" fill=\"none\">\n                <path fill-rule=\"evenodd\" clip-rule=\"evenodd\"\n                    d=\"M10.4713 7.52852C10.7317 7.78885 10.7317 8.21098 10.4713 8.47132L6.70005 12.2426C6.43973 12.5029 6.01763 12.5029 5.75727 12.2426C5.49693 11.9822 5.49693 11.5601 5.75727 11.2997L9.05712 7.99992L5.75727 4.70008C5.49693 4.43974 5.49693 4.01762 5.75727 3.75728C6.01763 3.49692 6.43973 3.49692 6.70005 3.75728L10.4713 7.52852Z\"\n                    fill=\"currentColor\" />\n            </svg>\n        </div>\n\n        <div class=\"carousel-container\" id=\"zyBlogCarousel\">\n            {% assign shown_count = 0 %}\n            {% assign blog_handle = \"magazine\" %}\n            {% assign all_articles = blogs[\"magazine\"].articles | sort: 'published_at' | reverse %}\n\n            {% if all_articles.size > 0 %}\n            {% for post in all_articles %}\n            {% if shown_count < 10 %}\n            <div class=\"carousel-item\">\n                <a href=\"{{ post.url }}\">\n                    {% if post.image %}\n                    <div class=\"image-wrapper\">\n                        <img src=\"{{ post.image | img_url: '400x' }}\" alt=\"{{ post.title }}\">\n                    </div>\n                    {% else %}\n                    <div style=\"width:100%;height:200px;background-color:#f0f0f0;\"></div>\n                    {% endif %}\n                    <h3>{{ post.title }}</h3>\n                </a>\n            </div>\n            {% assign shown_count = shown_count | plus: 1 %}\n            {% endif %}\n            {% endfor %}\n            {% else %}\n            <div class=\"carousel-item\">\n                <div style=\"width:100%;height:200px;background-color:#f0f0f0;\"></div>\n                <h3>Nenhum artigo encontrado</h3>\n            </div>\n            {% endif %}\n        </div>\n    </div>\n</div>\n\n<script>\n    function scrollCarousel(direction) {\n        const container = document.getElementById('zyBlogCarousel');\n        const scrollAmount = 260;\n        container.scrollBy({\n            left: direction * scrollAmount,\n            behavior: 'smooth'\n        });\n    }\n\n    document.addEventListener('DOMContentLoaded', function () {\n        const container = document.getElementById('zyBlogCarousel');\n        const buttons = document.querySelectorAll('.carousel-button');\n\n        if (container && container.scrollWidth <= container.clientWidth) {\n            buttons.forEach(button => button.style.display = 'none');\n        }\n    });\n</script>",
