s@1.6.0/dist/addons/cleave-phone.pt.js\"></script>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function () {\n    const tabButtons = document.querySelectorAll(\".tab-btn\");\n    const tabContents = document.querySelectorAll(\".tab-content\");\n\n    tabButtons.forEach(btn => {\n      btn.addEventListener(\"click\", function () {\n        const target = this.dataset.tab;\n\n        tabButtons.forEach(b => b.classList.remove(\"active\"));\n        tabContents.forEach(c => c.classList.remove(\"active\"));\n\n        this.classList.add(\"active\");\n        document.getElementById(target).classList.add(\"active\");\n      });\n    });\n\n    validateForm(\"#form1 form\");\n    validateForm(\"#form2 form\");\n\n    function validateForm(selector) {\n      const form = document.querySelector(selector);\n      if (!form) return;\n\n      const requiredFields = form.querySelectorAll(\n        'input[required], select[required]'); // Incluir selects\n\n      requiredFields.forEach(field => {\n        field.addEventListener('blur', () => {\n          validateField(field);\n        });\n\n        field.addEventListener('input', () => {\n          field.classList.remove('error');\n          const parent = field.closest('.option');\n          if (parent) {\n            if (field.type === 'checkbox') {\n              const label = parent.querySelector(`label[for=\"${field.id}\"]`);\n              if (label) {\n                const errorMessage = label.querySelector('.error-message');\n                if (errorMessage) errorMessage.remove();\n              }\n            } else {\n              const errorMessage = parent.querySelector('.error-message');\n              if (errorMessage) errorMessage.remove();\n            }\n          }\n        });\n\n        if (field.tagName === 'SELECT') {\n          field.addEventListener('change', () => {\n            validateField(field);\n          });\n        }\n      });\n\n      form.addEventListener('submit', function (e) {\n        let isValid = true;\n\n        requiredFields.forEach(field => {\n          if (!validateField(field)) {\n            isValid = false;\n          }\n        });\n\n        if (!isValid) {\n          e.preventDefault();\n        }\n      });\n    }\n\n    function validateField(field) {\n      const value = field.value.trim();\n      const isCheckbox = field.type === 'checkbox';\n      const isEmail = field.type === 'email';\n      const isSelect = field.tagName === 'SELECT';\n      const parent = field.closest('.option');\n      let errorMessageText = '';\n\n      if (parent) {\n        if (isCheckbox) {\n          const label = parent.querySelector(`label[for=\"${field.id}\"]`);\n          if (label) {\n            const existingErrorMessage = label.querySelector('.error-message');\n            if (existingErrorMessage) {\n              existingErrorMessage.remove();\n            }\n          }\n        } else {\n          const existingErrorMessage = parent.querySelector('.error-message');\n          if (existingErrorMessage) {\n            existingErrorMessage.remove();\n          }\n        }\n      }\n\n      let isValid = true;\n\n      if ((isCheckbox && !field.checked) || (!isCheckbox && !isSelect && value === '') || (isSelect &&\n          value === '')) {\n        isValid = false;\n        switch (field.id) {\n          case 'subscribername':\n            errorMessageText = 'Por favor, insira o nome do cliente.';\n            break;\n          case 'subscriberemail1':\n          case 'subscriberemail2':\n            errorMessageText = 'Por favor, insira o e-mail do cliente.';\n            break;\n          case 'zysms':\n            errorMessageText = 'Por favor, insira o número de telemóvel do cliente.';\n            break;\n          case 'subscriberconditions':\n          case 'subscriberconditions2':\n            errorMessageText = 'Campo obrigatório.';\n            break;\n          case 'zypassport':\n            errorMessageText = 'Por favor, insira o número do passaporte.';\n            break;\n          case 'stores_jornada':\n            errorMessageText = 'Por favor, selecione uma loja.';\n            break;\n          default:\n            errorMessageText = 'Campo obrigatório.';\n        }\n      } else if (isEmail && !isValidEmail(value)) {\n        isValid = false;\n        errorMessageText = 'Por favor, insira um endereço de e-mail válido.';\n      } else if (field.id === 'zysms') {\n        const phoneRegex = /^\\+351\\s?\\d{3}\\s?\\d{3}\\s?\\d{3}$/;\n        if (!phoneRegex.test(value)) {\n          isValid = false;\n          errorMessageText = 'Por favor, insira um número de telemóvel válido (+351XXXXXXXXX).';\n        }\n      }\n\n      if (!isValid) {\n        field.classList.add('error');\n        if (errorMessageText) {\n          const error = document.createElement('span');\n          error.classList.add('error-message');\n          error.textContent = errorMessageText;\n\n          if (isCheckbox) {\n            const label = parent.querySelector(`label[for=\"${field.id}\"]`);\n            if (label) {\n              label.appendChild(error);\n            }\n          } else {\n            if (parent) {\n              parent.appendChild(error);\n            }\n          }\n        }\n\n      } else {\n        field.classList.remove('error');\n        if (isCheckbox) {\n          const label = parent.querySelector(`label[for=\"${field.id}\"]`);\n          if (label) {\n            const existingErrorMessage = label.querySelector('.error-message');\n            if (existingErrorMessage) {\n              existingErrorMessage.remove();\n            }\n          }\n        } else {\n          const existingErrorMessage = parent.querySelector('.error-message');\n          if (existingErrorMessage) {\n            existingErrorMessage.remove();\n          }\n        }\n      }\n\n      return isValid;\n    }\n\n    function isValidEmail(email) {\n      return /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/.test(email);\n    }\n\n    new Cleave('#zysms', {\n      phone: true,\n      phoneRegionCode: 'PT',\n      prefix: '+351'\n    });\n  });\n</script>",
