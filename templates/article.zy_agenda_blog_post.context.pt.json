back-message');\n  \n    if (shareButton) {\n      shareButton.addEventListener('click', async () => {\n        const shareUrl = shareButton.dataset.shareUrl;\n        const shareTitle = shareButton.dataset.shareTitle;\n        const shareTextBase = 'Saiba tudo sobre \"';\n        const shareText = `${shareTextBase}${shareTitle}\" na Magazine da Zippy.`;\n  \n        if (navigator.share) {\n          try {\n            await navigator.share({\n              title: shareTitle,\n              text: shareText,\n              url: shareUrl,\n            });\n            shareFeedback.textContent = 'Conteúdo partilhado com sucesso!';\n          } catch (error) {\n            if (error.name !== 'AbortError') {\n              shareFeedback.textContent = 'Ocorreu um erro ao partilhar.';\n              console.error('Erro ao partilhar:', error);\n            } else {\n              shareFeedback.textContent = ''; \n            }\n          }\n        } else {\n        \n          try {\n            await navigator.clipboard.writeText(shareUrl);\n            shareFeedback.textContent = 'Link do artigo copiado para a área de transferência!';\n          } catch (err) {\n            shareFeedback.textContent = 'Não foi possível copiar o link.';\n            console.error('Erro ao copiar texto:', err);\n          }\n        }\n      });\n  \n      shareButton.addEventListener('keydown', async (event) => {\n        if (event.key === 'Enter' || event.key === ' ') {\n          event.preventDefault(); \n          shareButton.click(); \n        }\n      });\n    }\n  });\n  </script>",
