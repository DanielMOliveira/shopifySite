< ca.length; i++) {\n      let c = ca[i];\n      while (c.charAt(0) === \" \") {\n        c = c.substring(1);\n      }\n      if (c.indexOf(cookieName) === 0) {\n        return c.substring(cookieName.length, c.length);\n      }\n    }\n    return \"\";\n  }\n\n  const utmSource = getParameterByName(\"utm_source\");\n\n  if (utmSource === \"pos\") {\n    setCookie(\"hideBanner\", \"true\", 3650);\n  }\n\n  if (getCookie(\"hideBanner\") === \"true\") {\n    const banner = document.getElementById(\"zyTCB-BF\");\n    if (banner) {\n      banner.style.display = \"none\";\n    }\n  }\n</script>\n{% endif %}"
