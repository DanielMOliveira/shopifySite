;\n\n            if (swiper.isEnd) {\n              if (diff < 0) {\n                e.preventDefault();\n                scrollToNextBanner(nextBanner);\n              } else if (diff > 0) {\n                swiper.slideTo(2, 300);\n              }\n            }\n          };\n\n          lastSlide.addEventListener(\"touchstart\", touchStartHandler, {\n            passive: false,\n          });\n          lastSlide.addEventListener(\"touchmove\", touchMoveHandler, {\n            passive: false,\n          });\n        } else {\n          const wheelHandler = (e) => {\n            const isScrollingDown = e.deltaY > 0;\n\n            if (isScrollingDown && swiper.isEnd) {\n              e.preventDefault();\n              scrollToNextBanner(nextBanner);\n            }\n          };\n\n          lastSlide.addEventListener(\"wheel\", wheelHandler);\n        }\n      }\n    };\n\n    setupLastSlideEvents();\n\n    // Debounce function para evitar múltiplas chamadas durante o resize\n    const debounce = (func, wait) => {\n      let timeout;\n      return function executedFunction(...args) {\n        const later = () => {\n          clearTimeout(timeout);\n          func(...args);\n        };\n        clearTimeout(timeout);\n        timeout = setTimeout(later, wait);\n      };\n    };\n\n    // Handler do resize com debounce\n    const handleResize = debounce(() => {\n      const currentIsMobile = isMobile();\n\n      // Atualiza o Swiper apenas se necessário\n      if ((currentIsMobile && swiper.params !== mobileConfig) || (!currentIsMobile && swiper.params !== desktopConfig)) {\n        // Destroy o Swiper atual\n        swiper.destroy(true, true);\n\n        // Recria o Swiper com as configurações apropriadas\n        swiper = new Swiper(\".swiper-container\", currentIsMobile ? mobileConfig : desktopConfig);\n\n        // Reconfigura os eventos do último slide\n        setupLastSlideEvents();\n      }\n    }, 250);\n\n    // Listener para mudanças no tamanho da janela